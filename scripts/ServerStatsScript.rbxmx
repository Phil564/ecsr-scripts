					<string name="Name">ServerStats</string>
					<string name="ScriptGuid"></string>
					<ProtectedString name="Source"><![CDATA[--format function to maintain zeros(for example 0:2:5:10 -> 00:02:05:10)
function f(text)
	return string.format("%02i", text)
end

function toText(value)
	--a day has 86400 seconds
	local days = math.floor(value/86400)
	value = value % 86400
	local hours = math.floor(value/3600) 
	value = value % 3600
	local minutes = math.floor(value/60) 
	value = value % 60
	local seconds = value 
	
	return f(days)..":"..f(hours)..":"..f(minutes)..":"..f(seconds)
end

--check every second
while true do 
	--seconds since the server started
	wait(1)
	local seconds = workspace.DistributedGameTime
	local text = toText(seconds)
	game:GetService("Workspace")._ServerStats.CurrentServerAge.Value = text
	game:GetService("Workspace")._ServerStats.CurrentJobId.Value = game.JobId
end]]></ProtectedString>
