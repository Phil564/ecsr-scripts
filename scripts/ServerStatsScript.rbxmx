						<string name="ScriptGuid"></string>
						<ProtectedString name="Source"><![CDATA[local gui = script.Parent 

--check every second
while true do 
	--seconds since the server started
	wait(1)
	gui.ServerAge.Text = "Server Age: " .. game:GetService("Workspace")._ServerStats.CurrentServerAge.Value
	gui.PlaceVersion.Text = "Place Version: " .. game.PlaceVersion
	gui.PlaceId.Text = "PlaceId: " .. game.PlaceId
	gui.JobId.Text = "JobId: " .. game:GetService("Workspace")._ServerStats.CurrentJobId.Value
end ]]></ProtectedString>
